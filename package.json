    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.29",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.14.1",
    "@protobuf-ts/grpc-transport": "^2.11.1",
    "@protobuf-ts/grpcweb-transport": "^2.11.1",
    "@stanterprise/protobuf": "^0.0.19"
  }
