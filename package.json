{
  "name": "@stanterprise/playwright-reporter",
  "version": "0.0.8",
  "description": "Custom Playwright test reporter that sends test results to Stanterprise via gRPC",
  "homepage": "https://github.com/stanterprise/stanterprise-playwright-reporter#readme",
  "bugs": {
    "url": "https://github.com/stanterprise/stanterprise-playwright-reporter/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stanterprise/stanterprise-playwright-reporter.git"
  },
  "license": "ISC",
  "author": "Stanterprise Team",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "playwright",
    "reporter",
    "test",
    "testing",
    "stanterprise",
    "grpc",
    "playwright-reporter"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@playwright/test": ">=1.20.0"
  },
  "scripts": {
